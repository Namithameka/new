list=[1,2,3,4,5]
list.append(7)
print(list)
list.sort()
print(list)
list.extend([1,2,3,5,6,7])
print(list)
list.insert(1,50)
print(list)
list.insert(4,39)
print(list)
list.reverse()
print(list)
list.sort(reverse=False)
print(list)
list.sort(reverse=True)
print(list)

print(list.count(50))
print(list.count(5))
print(list[1])
print(list[0])
